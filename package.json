{
  "name": "available-domains",
  "version": "1.0.0",
  "description": "Detect available domains from provided entries",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "available-domain": "./src/cli",
    "available-domains": "./src/cli"
  },
  "main": "./index",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rimraf src/**/*.{js,d.ts} index.{js,d.ts}",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "ts-check": "tsc -p ./tsconfig.json --noEmit",
    "test": "npm run ts-check && npm run lint",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/rangoo94/available-domains#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rangoo94/available-domains.git"
  },
  "bugs": {
    "url": "https://github.com/rangoo94/available-domains/issues"
  },
  "keywords": [
    "whois",
    "dns",
    "domain",
    "available",
    "availability",
    "domains",
    "check",
    "hostname",
    "free"
  ],
  "author": {
    "name": "Dawid Rusnak",
    "email": "dawid@drcode.pl",
    "url": "https://www.drcode.pl"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "src/*.js",
    "src/*.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "is-valid-domain": "^0.1.6",
    "p-map": "^4.0.0",
    "progress": "^2.0.3",
    "whois": "^2.13.9"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^17.0.29",
    "@types/progress": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  }
}
